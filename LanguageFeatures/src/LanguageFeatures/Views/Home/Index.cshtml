@model IEnumerable<string>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Language Features</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
</head>
<body>
    <ul>
        @foreach (string item in Model)
        {
            <li>@item</li>
        }
        <li>@ViewContext.RouteData.Values["action"].ToString()</li>
    </ul>    
        <a class="btn btn-primary" asp-action="Index">Index</a>    
        <a class="btn btn-primary" asp-action="ExtensionMethods">Extension Method</a>
        <a class="btn btn-danger" asp-action="AsyncMethods">Async Method</a>    

</body>
</html>
